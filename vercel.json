{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/$",
      "dest": "/docs"
    },
    {
      "src": "/docs",
      "dest": "/public/docs.html"
    },
    {
      "src": "/swagger.json",
      "dest": "/public/swagger.json"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
